{
  "name": "zet_asociatie",
  "version": "0.1.1",
  "description": "Aplicatie de administrare membri",
  "author": "Zet Securty",
  "private": true,
  "scripts": {
    "build": "next build --webpack && node scripts/copy-standalone-static.js",
    "dev": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "migrate:local": "node scripts/migrate.js",
    "electron:dev": "electron ./electron/main.js",
    "version:bump": "npm version patch --no-git-tag-version",
    "electron:build": "npm run version:bump && electron-builder -w",
    "electron:build:mac": "npm run version:bump && electron-builder --mac",
    "electron:pack": "electron-builder --dir -w",
    "electron:pack:mac": "electron-builder --dir --mac"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "1.3.1",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.454.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "pg": "^8.13.0",
    "react": "19.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "xlsx": "0.18.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "postcss": "^8.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.zet.asociatie",
    "productName": "Zet Asociatie",
    "directories": {
      "buildResources": "build",
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "public/**/*",
      "package.json",
      "scripts/**/*",
      "db/**/*",
      "node_modules/pg/**/*",
      "node_modules/next/**/*",
      "node_modules/react/**/*",
      "node_modules/react-dom/**/*",
      "node_modules/styled-jsx/**/*",
      "node_modules/@next/**/*",
      "node_modules/@swc/**/*",
      "node_modules/caniuse-lite/**/*",
      "node_modules/postcss/**/*",
      "node_modules/nanoid/**/*",
      "node_modules/busboy/**/*",
      "node_modules/watchpack/**/*"
    ],
    "extraResources": [
      {
        "from": "resources/postgres-win",
        "to": "postgres-win",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      ".next/standalone/**",
      "node_modules/pg/**",
      "node_modules/next/**",
      "node_modules/react/**",
      "node_modules/react-dom/**",
      "node_modules/styled-jsx/**",
      "node_modules/@next/**",
      "node_modules/@swc/**"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerLanguages": ["en_US"],
      "deleteAppDataOnUninstall": false
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.png"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.business",
      "icon": "build/icon.png"
    }
  }
}
